########## Git ##########
alias gs="git status"
alias gl="git log"
alias wrk="git checkout"
alias gcb="git checkout -b"

########## Bash ##########
alias sb="source ~/.bash_aliases"
alias eb="vim ~/.bash_aliases"
alias cb="cat ~/.bash_aliases"

########## LS ##########
alias ls="ls -G"
alias cls="clear; ls"
alias clr="clear; ls -a"
